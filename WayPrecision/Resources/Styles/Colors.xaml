<?xml version="1.0" encoding="UTF-8" ?>
<?xaml-comp compile="true" ?>
<ResourceDictionary
    xmlns="http://schemas.microsoft.com/dotnet/2021/maui"
    xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml">

    <!-- Note: For Android please see also Platforms\Android\Resources\values\colors.xml -->

    <Color x:Key="Primary">#dddddd</Color>
    <Color x:Key="PrimaryDark">#ac99ea</Color>
    <Color x:Key="PrimaryDarkText">#242424</Color>
    <Color x:Key="Secondary">#DFD8F7</Color>
    <Color x:Key="SecondaryDarkText">#9880e5</Color>
    <Color x:Key="Tertiary">#2B0B98</Color>

    <Color x:Key="White">White</Color>
    <Color x:Key="Black">Black</Color>
    <Color x:Key="Magenta">#D600AA</Color>
    <Color x:Key="MidnightBlue">#190649</Color>
    <Color x:Key="OffBlack">#1f1f1f</Color>

    <Color x:Key="Primary50">#f9e5e5</Color>
    <Color x:Key="Primary100">#f2cccc</Color>
    <Color x:Key="Primary200">#ecb2b2</Color>
    <Color x:Key="Primary300">#d96666</Color>
    <Color x:Key="Primary500">#c00000</Color>
    <Color x:Key="Primary600">#900000</Color>
    <Color x:Key="Primary700">#660000</Color>

    <Color x:Key="Gray100">#f5f5f5</Color>
    <Color x:Key="Gray200">#f2f2f2</Color>
    <Color x:Key="Gray300">#dddddd</Color>
    <Color x:Key="Gray400">#939393</Color>
    <Color x:Key="Gray500">#6e6e6e</Color>
    <Color x:Key="Gray700">#333333</Color>

    <Color x:Key="SemanticBlue300">#ecf9fb</Color>
    <Color x:Key="SemanticBlue500">#c8e5f8</Color>
    <Color x:Key="SemanticBlue700">#006db5</Color>
    <Color x:Key="SemanticBlueBright">#2c93f2</Color>
    <Color x:Key="SemanticGreen300">#eaf9ea</Color>
    <Color x:Key="SemanticGreen500">#c4e4c4</Color>
    <Color x:Key="SemanticGreen700">#3f7200</Color>
    <Color x:Key="SemanticGrey500">#dddddd</Color>
    <Color x:Key="SemanticOrange300">#fff5e1</Color>
    <Color x:Key="SemanticOrange500">#fbe4bc</Color>
    <Color x:Key="SemanticOrange700">#bc5000</Color>
    <Color x:Key="SemanticPurple500">#dcc0f8</Color>
    <Color x:Key="SemanticPurple700">#7320c2</Color>
    <Color x:Key="SemanticRed300">#fbeeed</Color>
    <Color x:Key="SemanticRed500">#f5b4b4</Color>
    <Color x:Key="SemanticRed700">#dd0404</Color>

    <SolidColorBrush x:Key="PrimaryBrush" Color="{StaticResource Primary}" />
    <SolidColorBrush x:Key="SecondaryBrush" Color="{StaticResource Secondary}" />
    <SolidColorBrush x:Key="TertiaryBrush" Color="{StaticResource Tertiary}" />
    <SolidColorBrush x:Key="WhiteBrush" Color="{StaticResource White}" />
    <SolidColorBrush x:Key="BlackBrush" Color="{StaticResource Black}" />

    <SolidColorBrush x:Key="Primary50Brush" Color="{StaticResource Primary50}" />
    <SolidColorBrush x:Key="Primary100Brush" Color="{StaticResource Primary100}" />
    <SolidColorBrush x:Key="Primary200Brush" Color="{StaticResource Primary200}" />
    <SolidColorBrush x:Key="Primary300Brush" Color="{StaticResource Primary300}" />
    <SolidColorBrush x:Key="Primary500Brush" Color="{StaticResource Primary500}" />
    <SolidColorBrush x:Key="Primary600Brush" Color="{StaticResource Primary600}" />
    <SolidColorBrush x:Key="Primary700Brush" Color="{StaticResource Primary700}" />

    <SolidColorBrush x:Key="Gray100Brush" Color="{StaticResource Gray100}" />
    <SolidColorBrush x:Key="Gray200Brush" Color="{StaticResource Gray200}" />
    <SolidColorBrush x:Key="Gray300Brush" Color="{StaticResource Gray300}" />
    <SolidColorBrush x:Key="Gray400Brush" Color="{StaticResource Gray400}" />
    <SolidColorBrush x:Key="Gray500Brush" Color="{StaticResource Gray500}" />
    <SolidColorBrush x:Key="Gray700Brush" Color="{StaticResource Gray700}" />

    <SolidColorBrush x:Key="SemanticBlue300Brush" Color="{StaticResource SemanticBlue300}" />
    <SolidColorBrush x:Key="SemanticBlue500Brush" Color="{StaticResource SemanticBlue500}" />
    <SolidColorBrush x:Key="SemanticBlue700Brush" Color="{StaticResource SemanticBlue700}" />
    <SolidColorBrush x:Key="SemanticBlueBrightBrush" Color="{StaticResource SemanticBlueBright}" />
    <SolidColorBrush x:Key="SemanticGreen300Brush" Color="{StaticResource SemanticGreen300}" />
    <SolidColorBrush x:Key="SemanticGreen500Brush" Color="{StaticResource SemanticGreen500}" />
    <SolidColorBrush x:Key="SemanticGreen700Brush" Color="{StaticResource SemanticGreen700}" />
    <SolidColorBrush x:Key="SemanticGrey500Brush" Color="{StaticResource SemanticGrey500}" />
    <SolidColorBrush x:Key="SemanticOrange300Brush" Color="{StaticResource SemanticOrange300}" />
    <SolidColorBrush x:Key="SemanticOrange500Brush" Color="{StaticResource SemanticOrange500}" />
    <SolidColorBrush x:Key="SemanticOrange700Brush" Color="{StaticResource SemanticOrange700}" />
    <SolidColorBrush x:Key="SemanticPurple500Brush" Color="{StaticResource SemanticPurple500}" />
    <SolidColorBrush x:Key="SemanticPurple700Brush" Color="{StaticResource SemanticPurple700}" />
    <SolidColorBrush x:Key="SemanticRed300Brush" Color="{StaticResource SemanticRed300}" />
    <SolidColorBrush x:Key="SemanticRed500Brush" Color="{StaticResource SemanticRed500}" />
    <SolidColorBrush x:Key="SemanticRed700Brush" Color="{StaticResource SemanticRed700}" />
</ResourceDictionary>